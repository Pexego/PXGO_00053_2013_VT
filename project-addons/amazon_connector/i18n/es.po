#. module: amazon_connector
#: model:mail.template,body_html:amazon_connector.send_amazon_settlement
msgid "\n"
"            <p>The Amazon settlement <strong>${object.name} - ${object.marketplace_id.name} </strong> has ended.</p>\n"
"            <p>ERRORS: ${ctx.get('error_count')}</strong></p>\n"
"            <p>RECONCILED: ${ctx.get('reconciled_count')}</p>\n"
"         \n"
"        "
msgstr "\n"
"            <p style=\"margin:0px 0px 9px 0px;font-size:13px;font-family:&quot;Lucida Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">El informe de pago de amazon <strong style=\"font-weight:bold;\">${object.name} - ${object.marketplace_id.name} </strong> ha terminado.</p>\n"
"            <p style=\"margin:0px 0px 9px 0px;font-size:13px;font-family:&quot;Lucida Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">ERRORES: ${ctx.get('error_count')}</p>\n"
"            <p style=\"margin:0px 0px 9px 0px;font-size:13px;font-family:&quot;Lucida Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">CONCILIADAS: ${ctx.get('reconciled_count')}</p>\n"
"         \n"
"        "

#. module: amazon_connector
#: model:mail.template,body_html:amazon_connector.send_mail_errors_amazon
