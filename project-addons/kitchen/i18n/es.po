#. module: kitchen
#: model:mail.template,body_html:kitchen.send_mail_cancel_customization
msgid "\n"
"            <p> The customization ${object.name} - ${object.partner_id.name or ''} has been cancelled.</p>\n"
"            % if ctx.get('picking_message'):\n"
"                <p> If you want the order (${object.order_id.name or ''}) to be shipped without customizing it, please, remove the 'Not sync' check from the picking.\n"
"                If, on the other hand, you want to cancel it or change the customization, cancel the order and confirm it again if necessary .</p>\n"
"                <p>\n"
"            % endif\n"
"            <p>Best regards.</p>\n"
"         \n"
"        "
msgstr "<p style=\"margin:0px 0px 9px 0px;font-size:13px;font-family:&quot;Lucida Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\"> La personalización ${object.name} - ${object.partner_id.name or ''} ha sido cancelada.</p>\n"
"            % if ctx.get('picking_message'):\n"
"                <p style=\"margin:0px 0px 9px 0px;font-size:13px;font-family:&quot;Lucida Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\"> Si quieres que el pedido(${object.order_id.name or ''}) sea enviado sin personalizar,por favor, elimina el check de 'No sincronizable' del albarán.\n"
"                Si por el contrario quieres cancelarlo o cambiar la personalización, cancela el pedido y tramítalo de nuevo si fuera necesario.</p>\n"
"                <p style=\"margin:0px 0px 9px 0px;font-size:13px;font-family:&quot;Lucida Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">\n"
"            % endif\n"
"            </p><p style=\"margin:0px 0px 9px 0px;font-size:13px;font-family:&quot;Lucida Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">Saludos.</p>"

#. module: kitchen
#: model:mail.template,body_html:kitchen.send_mail_old_previews
msgid "\n"
"            <p> The customization ${object.name} - ${object.partner_id.name or ''} has old preview/s.</p>\n"
"            <p> The information of this request is the following:</p>\n"
"            <ul>\n"
"                <li>Customization name: <strong>${object.name}</strong></li>\n"
"                <li>Commercial: <strong>${object.commercial_id.name}</strong></li>\n"
"                <li>Partner: <strong>${object.partner_id.name | safe}</strong></li>\n"
"                <li>Old Preview Products: ctx.get('old_previews')</strong></li>\n"
"            </ul>\n"
"            <p>Best regards.</p>\n"
"         \n"
"        "
msgstr "\n"
"            <p> La personalización ${object.name} - ${object.partner_id.name or ''} tiene previews antiguas/s.</p>\n"
"            <p> La información de esta petición es la siguiente:</p>\n"
"            <ul>\n"
"                <li>Nombre: <strong>${object.name}</strong></li>\n"
"                <li>Comercial: <strong>${object.commercial_id.name}</strong></li>\n"
"                <li>Cliente: <strong>${object.partner_id.name | safe}</strong></li>\n"
"                <li>Previews Antiguas: ctx.get('old_previews')</strong></li>\n"
"            </ul>\n"
"            <p>Saludos.</p>\n"
"         \n"
"        "

#. module: kitchen
#: model:mail.template,body_html:kitchen.send_mail_to_commercials_customization_done
msgid "\n"
"            <p>Customization <strong>${object.name}</strong> has been completed.</p>\n"
"            <p>The information of this customization is the following:</p>\n"
"            <ul>\n"
"                <li>Partner: <strong>${object.partner_id.name or ''}</strong></li>\n"
"                <li>Order: <strong>${object.order_id.name or ''}</strong></li>\n"
"                <li>Products and quantities: <strong>${object.products_qty_format} </li>\n"
"                <li>Responsible for customization: <strong>${object.user} </li>\n"
"            </ul>\n"
"            <p>Shortly, the order <strong>${object.order_id.name or ''} -- ${ctx['picking_name']}</strong> will be prepared for the next shipment.</p>\n"
"            <p>Best regards.</p>\n"
"         \n"
"        "
msgstr "<p style=\"margin:0px 0px 9px 0px;font-size:13px;font-family:&quot;Lucida Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">La Personalización <strong style=\"font-weight:bold;\">${object.name}</strong> ha terminado.</p>\n"
"            <p style=\"margin:0px 0px 9px 0px;font-size:13px;font-family:&quot;Lucida Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">La información de la personalización es la siguiente::</p>\n"
"            <ul style=\"margin:0px 0 9px 0;\"><li>Cliente: <strong style=\"font-weight:bold;\">${object.partner_id.name or ''}</strong></li><li>Pedido: <strong style=\"font-weight:bold;\">${object.order_id.name or ''}</strong></li><li>Productos y cantidades: <strong style=\"font-weight:bold;\">${object.products_qty_format} </strong></li><strong style=\"font-weight:bold;\">\n"
"                <li>Responsable de la personalización: <strong style=\"font-weight:bold;\">${object.user} </strong></li><strong style=\"font-weight:bold;\">\n"
"            </strong></strong></ul><strong style=\"font-weight:bold;\"><strong style=\"font-weight:bold;\">\n"
"            <p style=\"margin:0px 0px 9px 0px;font-size:13px;font-family:&quot;Lucida Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">En breve se procederá a la preparación del pedido <strong style=\"font-weight:bold;\">${object.order_id.name or ''} -- ${ctx['picking_name']}</strong> para su posterior envío.</p>\n"
"            <p style=\"margin:0px 0px 9px 0px;font-size:13px;font-family:&quot;Lucida Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">Saludos.</p></strong></strong>"

#. module: kitchen
#: model:mail.template,body_html:kitchen.send_mail_to_kitchen_customization_sent
msgid "\n"
"            <p>Good morning,</p>\n"
"            <p>There is a new customization request <strong>${object.name}</strong> The information of this request is the following:</p>\n"
"            <ul>\n"
"                <li>Customization name: <strong>${object.name}</strong></li>\n"
"                <li>Commercial: <strong>${object.commercial_id.name}</strong></li>\n"
"                <li>Partner: <strong>${object.partner_id.name or ''}</strong></li>\n"
"                <li>Order: <strong>${object.order_id.name or ''}</strong></li>\n"
"                <li>Request: <strong>${object.products_qty_format} </li>\n"
"                <li>Comments: <strong>${object.comments or ''}</strong></li>\n"
"            </ul>\n"
"            <p>Best regards.</p>\n"
"         \n"
"        "
msgstr "<p style=\"margin:0px 0px 9px 0px;font-size:13px;font-family:&quot;Lucida Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">Buenos días,</p>\n"
"            <p style=\"margin:0px 0px 9px 0px;font-size:13px;font-family:&quot;Lucida Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">Hay una nueva petición de personalización <strong style=\"font-weight:bold;\">${object.name}</strong>. La información de esta petición es la siguiente:</p>\n"
"            <ul style=\"margin:0px 0 9px 0;\">\n"
"                <li>Personalización: <strong style=\"font-weight:bold;\">${object.name}</strong></li>\n"
"                <li>Comercial: <strong style=\"font-weight:bold;\">${object.commercial_id.name}</strong></li>\n"
"                <li>Cliente: <strong style=\"font-weight:bold;\">${object.partner_id.name or ''}</strong></li>\n"
"                <li>Pedido: <strong style=\"font-weight:bold;\">${object.order_id.name or ''}</strong></li>\n"
"                <li>Productos y cantidades: <strong style=\"font-weight:bold;\">${object.products_qty_format} </strong></li><strong style=\"font-weight:bold;\">\n"
"                <li>Comentarios: <strong style=\"font-weight:bold;\">${object.comments or ''}</strong></li>\n"
"            </strong></ul><strong style=\"font-weight:bold;\">\n"
"            <p style=\"margin:0px 0px 9px 0px;font-size:13px;font-family:&quot;Lucida Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">Saludos.</p></strong>"

#. module: kitchen
#: model:mail.template,body_html:kitchen.send_mail_to_commercials_date_planned_changed
msgid "\n"
"            <p>The expected date of customization <strong>${object.name}</strong> has changed.</p>\n"
"            <p>The new expected date is <strong>${ctx.get('date_planned') or ''}</strong></p>\n"
"            <p>Best regards.</p>\n"
"         \n"
"        "
msgstr "<p>La fecha esperada de la personalización <strong>${object.name}</strong> ha cambiado.</p>\n"
"            <p>La nueva fecha es <strong>${ctx.get('date_planned') or ''}</strong></p>\n"
"            <p>Saludos.</p>"

