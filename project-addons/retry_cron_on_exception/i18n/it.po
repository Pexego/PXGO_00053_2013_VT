msgstr "Tempo per riprovare"

#. module: retry_cron_on_exception
#: model:ir.model,name:retry_cron_on_exception.model_ir_cron
msgid "Scheduled Actions"
msgstr "Scheduled Actions"

#. module: retry_cron_on_exception
#: model:ir.model.fields,help:retry_cron_on_exception.field_ir_cron_max_retries
msgid "The cron will fail if the number of tries reach the max. retries.\n"
