<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">
    <record model="transportation.transporter" id="palletized_shipping_transporter">
        <field name="name">Envío Paletizado</field>
    </record>
    <record model="transportation.service" id="palletized_shipping_service">
        <field name="name">Envío Paletizado</field>
        <field name="transporter_ids" eval="[(4, ref('advise_special_shipping_costs.palletized_shipping_transporter'))]"/>
    </record>

    <record id="advise_special_shipping_email" model="ir.config_parameter">
        <field name="key">advise_special_shipping_email</field>
        <field name="value">operaciones@visiotechsecurity.com</field>
    </record>
</odoo>
