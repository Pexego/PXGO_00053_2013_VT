<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">
    <record model="res.partner" id="palletized_shipping_transporter">
        <field name="name">Envío Paletizado</field>
        <field name="customer" eval="False"/>
        <field name="is_transporter" eval="True"/>
    </record>
    <record model="delivery.carrier" id="palletized_shipping_service">
        <field name="name">Envío Paletizado</field>
        <field name="partner_id" eval="ref('advise_special_shipping_costs.palletized_shipping_transporter')"/>
        <field name="product_id" ref="delivery.product_product_delivery"/>
    </record>

    <record id="advise_special_shipping_email" model="ir.config_parameter">
        <field name="key">advise_special_shipping_email</field>
        <field name="value">operaciones@visiotechsecurity.com</field>
    </record>

    <record id="group_operations" model="res.groups">
            <field name="name">Operations</field>
    </record>
</odoo>
