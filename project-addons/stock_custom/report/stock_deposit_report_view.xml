<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="stock_deposit_report_tree" model="ir.ui.view">
        <field name="name">stock.deposit.report.tree</field>
        <field name="model">stock.deposit.report</field>
        <field name="arch" type="xml">
            <tree string="Stock Deposit Report">
                <field name="product_id"/>
                <field name="owner_id"/>
                <field name="location_id"/>
                <field name="product_qty"/>
                <field name="company_id"/>
            </tree>
        </field>
    </record>

    <record id="action_stock_deposit_report" model="ir.actions.act_window">
        <field name="name">Stock Deposit Report</field>
        <field name="res_model">stock.deposit.report</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree</field>
    </record>

</odoo>
