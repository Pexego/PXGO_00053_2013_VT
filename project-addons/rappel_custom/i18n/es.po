#. module: rappel_custom
#: model:mail.template,body_html:rappel_custom.rappel_mail_advice
msgid "<html>\n"
"<head>  \n"
"<meta http-equiv=\"Content-type\" content=\"text/html; charset=utf-8\">\n"
"                        <title>Rappels ${ctx['partner_name']}</title>\n"
"                        <style>\n"
"                            span.oe_mail_footer_access {\n"
"                                display:block;\n"
"                                text-align:center;\n"
"                                color:grey;\n"
"                            }\n"
"                        </style>\n"
"</head>\n"
"\n"
"\n"
"<body>\n"
"     <div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: #FFF; \">\n"
"                            <p>Dear Sir or Madam,</p>\n"
"                            <div>\n"
"                                % for line in ctx['values']:\n"
"                                <p>We are writing to inform you of the current status of your rebate program <em>${line.concepto}</em>.\n"
"                                    % if line.advice_timing == 'fixed':\n"
"                                        With ${line.timing} days left of\n"
"                                    % endif\n"
"                                    % if line.advice_timing == 'variable':\n"
"                                        After ${line.timing}% of\n"
"                                    % endif\n"
"                                  the quarter your total purchase amount for this period is:\n"
"                                </p>\n"
"                                %if line.curr_qty:\n"
"                                    <p>Amount: ${line.curr_qty} €</p>\n"
"                                %endif\n"
"                                % endfor\n"
"                            </div>\n"
"                            <p>If you have any questions, please do not hesitate to contact your account manager ${object.user_id.name}.</p>\n"
"                            <p>Best regards, Visiotech.</p>\n"
"                            <div style=\"width: 375px; margin: 0px; padding: 0px; background-color: #8E0000; border-top-left-radius: 5px 5px; border-top-right-radius: 5px 5px; background-repeat: repeat no-repeat;\">\n"
"                                <h3 style=\"margin: 0px; padding: 2px 14px; font-size: 12px; color: #DDD;\"><span style=\"text-transform: uppercase;\">VISIOTECH</span></h3>\n"
"                            </div>\n"
"                            <div style=\"width: 347px; margin: 0px; padding: 5px 14px; line-height: 16px; background-color: #F2F2F2;\">\n"
"                                <span style=\"color: #222; margin-bottom: 5px; display: block; \">\n"
"                                % if object.company_id.street:\n"
"                                    ${object.company_id.street}<br>\n"
"                                % endif\n"
"                                % if object.company_id.street2:\n"
"                                    ${object.company_id.street2}<br>\n"
"                                % endif\n"
"                                % if object.company_id.city or object.company_id.zip:\n"
"                                    ${object.company_id.zip} ${object.company_id.city}<br>\n"
"                                % endif\n"
"                                % if object.company_id.country_id:\n"
"                                    ${object.company_id.state_id and ('%s, ' % object.company_id.state_id.name) or ''} ${object.company_id.country_id.name or ''}<br>\n"
"                                % endif\n"
"                                </span>\n"
"                                % if object.company_id.phone:\n"
"                                    <div style=\"margin-top: 0px; margin-right: 0px; margin-bottom: 0px; margin-left: 0px; padding-top: 0px; padding-right: 0px; padding-bottom: 0px; padding-left: 0px; \">\n"
"                                        Phone:&nbsp; ${object.company_id.phone}\n"
"                                    </div>\n"
"                                % endif\n"
"                                % if object.company_id.website:\n"
"                                    <div>\n"
"                                        Web :&nbsp;<a href=\"${object.company_id.website}\">${object.company_id.website}</a>\n"
"                                    </div>\n"
"                                %endif\n"
"                                <p></p>\n"
"                            </div>\n"
"                        </div>\n"
"</body>\n"
"</html>"
msgstr "<html>\n"
"<head>  \n"
"<meta http-equiv=\"Content-type\" content=\"text/html; charset=utf-8\">\n"
"                        <title>Rappels ${ctx['partner_name']}</title>\n"
"                        <style>\n"
"                            span.oe_mail_footer_access {\n"
"                                display:block;\n"
"                                text-align:center;\n"
"                                color:grey;\n"
"                            }\n"
"                        </style>\n"
"</head>\n"
"\n"
"\n"
"<body>\n"
"     <div style=\"font-family: 'Lucica Grande', Ubuntu, Arial, Verdana, sans-serif; font-size: 12px; color: rgb(34, 34, 34); background-color: #FFF; \">\n"
"                            <p>Estimado Cliente:</p>\n"
"                            <div>\n"
"                                % for line in ctx['values']:\n"
"                                <p>En relación a su rapel <em>${line.concepto}</em>,\n"
"                                    % if line.advice_timing == 'fixed':\n"
"                                        y a falta de ${line.timing} días para la finalización del mismo,\n"
"                                    % endif\n"
"                                    % if line.advice_timing == 'variable':\n"
"                                        transcurrido el ${line.timing}% el periodo computable,\n"
"                                    % endif\n"
"                                  le informamos que la situación actual de su importe de compras a fecha de hoy es de:\n"
"                                </p>\n"
"                                %if line.curr_qty:\n"
"                                    <p>Cantidad: ${line.curr_qty} €</p>\n"
"                                %endif\n"
"                                % endfor\n"
"                            </div>\n"
"                            <p>Por favor, si esta información no es de su competencia, hágasela llegar al responsable de compras de su empresa para que tenga noticia de ella y, si tiene cualquier pregunta al respecto, no dude en contactar con su comercial habitual ${object.user_id.name}.</p>\n"
"                            <p>Reciba un cordial saludo.</p>\n"
"                            <div style=\"width: 375px; margin: 0px; padding: 0px; background-color: #8E0000; border-top-left-radius: 5px 5px; border-top-right-radius: 5px 5px; background-repeat: repeat no-repeat;\">\n"
"                                <h3 style=\"margin: 0px; padding: 2px 14px; font-size: 12px; color: #DDD;\"><span style=\"text-transform: uppercase;\">VISIOTECH</span></h3>\n"
"                            </div>\n"
"                            <div style=\"width: 347px; margin: 0px; padding: 5px 14px; line-height: 16px; background-color: #F2F2F2;\">\n"
"                                <span style=\"color: #222; margin-bottom: 5px; display: block; \">\n"
"                                % if object.company_id.street:\n"
"                                    ${object.company_id.street}<br>\n"
"                                % endif\n"
"                                % if object.company_id.street2:\n"
"                                    ${object.company_id.street2}<br>\n"
"                                % endif\n"
"                                % if object.company_id.city or object.company_id.zip:\n"
"                                    ${object.company_id.zip} ${object.company_id.city}<br>\n"
"                                % endif\n"
"                                % if object.company_id.country_id:\n"
"                                    ${object.company_id.state_id and ('%s, ' % object.company_id.state_id.name) or ''} ${object.company_id.country_id.name or ''}<br>\n"
"                                % endif\n"
"                                </span>\n"
"                                % if object.company_id.phone:\n"
"                                    <div style=\"margin-top: 0px; margin-right: 0px; margin-bottom: 0px; margin-left: 0px; padding-top: 0px; padding-right: 0px; padding-bottom: 0px; padding-left: 0px; \">\n"
"                                        Teléfono:&nbsp; ${object.company_id.phone}\n"
"                                    </div>\n"
"                                % endif\n"
"                                % if object.company_id.website:\n"
"                                    <div>\n"
"                                        Web :&nbsp;<a href=\"${object.company_id.website}\">${object.company_id.website}</a>\n"
"                                    </div>\n"
"                                %endif\n"
"                                <p></p>\n"
"                            </div>\n"
"                        </div>\n"
"</body>\n"
"</html>"

